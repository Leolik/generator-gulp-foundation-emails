{
  "name": "<%= kebabName %>",
  "version": "0.0.1",
  "description": "<%= name %> description",
  "author": "",
  "devDependencies": {<% if (htmlFramework !== 'heml') { %>
    "browser-sync": "^2.26.7",
    "del": "~5.1.0",
    "gulp": "~4.0.2",
    "gulp-concat": "~2.6.1",
    "gulp-inline-css": "~3.5.0",
    "gulp-imagemin": "^7.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-inline-source": "~4.0.0",
    "gulp-rename": "~2.0.0",<% } %><% if (sass) { %>
    "gulp-sourcemaps": "~2.6.5",
    "gulp-dart-sass": "^0.9.1",<% } %><% if (htmlFramework === 'pug') { %>
    "gulp-pug": "^4.0.1",<% } else if (htmlFramework === 'inky') { %>
    "inky": "^1.3.7",
    "gulp-replace": "^1.0.0",
    "siphon-media-query": "^1.0.0",
    "panini": "^1.6.3",<% } %><% if (htmlFramework === 'heml') { %>
    "heml": "^1.1.3"<% } else { %>
    "yargs": "^15.1.0"<% } %>
  },
  "scripts": {<% if (htmlFramework === 'heml') { %>
    "serve": "heml develop app/index.heml --open",
    "build": "heml build app/index.heml"<% } else { %>
    "serve": "gulp serve",
    "build": "gulp build --minify"<% } %>
  }
}
